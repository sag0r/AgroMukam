@model string

<div class="container cart-item-container pt-4">
    <div class="card rounded-0 delivery-option mb-3">
        <div class="card-body text-center">
            <h3 class="font-weight-normal">Ops! Something Went Wrong</h3>
            <h4 class="text-danger"> @Model </h4>
            <hr class="mt-3 mb-3"
                style="border: none; height: 1px; background: #eee;" />
            <button class="btn bg-greenish text-white shadow refresh-cart"> Refresh Cart </button>
        </div>
    </div>
</div>