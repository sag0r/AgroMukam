@model IList<Nop.Web.Models.Advertisements.AdvertisementViewModel>

<div class="ads-space">
    @if (Model.Count > 0)
    {
        @foreach (var ad in Model)
        {
            <a href="@ad.HyperLink">
                <img src="@ad.ImageUrl" alt="@ad.Caption" class="m-auto">
            </a>
        }
    }
    else
    {
        <a href="@Url.RouteUrl("ContactUs")">
            <img src="/Themes/Venture/Content/img/ads-space-greenish.jpg">
        </a>
    }
</div>